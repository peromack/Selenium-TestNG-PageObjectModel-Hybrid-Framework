<suite name="Import Cargo Automated Regression Test Suite">

  <listeners>
       <listener class-name="resources.SuiteListener"></listener>
       <listener class-name="resources.TestListener"></listener>
       <listener class-name="resources.GlobalTransformer"></listener>
  </listeners>
  
 <parameter name="Browser-Type"  value="Google Chrome" />
 <parameter name="Run-Type"  value="Local" />
 <parameter name="RunTime-Environment"  value="sat" />
 
 <test name="Actions Module: Test Suite">
  <classes>
   <class name="testsuite.ExamFindingsTestCase" />
  </classes>
 </test> 

 
